<?xml version="1.0" encoding="UTF-8"?>

<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.diduf.bd"
		versionCode = "36" 
        version   = "1.0.0">

    <name>DIDUF</name>

    <description>
		Destiny Investor and Distribution Forum (DIDUF)


    </description>

    <author href="http://phonegap.com" email="support@phonegap.com">
       Pavel Sarwar
    </author>
	


      
	  
	  
	<icon src="res/mipmap-ldpi/launcher.png" gap:platform="android" gap:qualifier="mdpi" />
	<icon src="res/mipmap-mdpi/launcher.png" gap:platform="android" gap:qualifier="mdpi" />
	<icon src="res/mipmap-hdpi/launcher.png" gap:platform="android" gap:qualifier="hdpi" />
	<icon src="res/mipmap-xhdpi/launcher.png" gap:platform="android" gap:qualifier="xhdpi" />
	<icon src="res/mipmap-xxhdpi/launcher.png" gap:platform="android" gap:qualifier="xxhdpi" />
	<icon src="res/mipmap-xxxhdpi/launcher.png" gap:platform="android" gap:qualifier="xxxhdpi" />
	




    
    <preference name="permissions"                value="none"/>
	
	
	
	






    <!-- Customize your app and platform with the preference element. -->
	<preference name="permissions" value="none"/>
    <preference name="phonegap-version"           value="cli-9.0.0" />           <!-- all: current version of PhoneGap -->
    <preference name="orientation"                value="default" />        <!-- all: default means both landscape and portrait are enabled -->
    <preference name="target-device"              value="universal" />      <!-- all: possible values handset, tablet, or universal -->
    <preference name="fullscreen"                 value="false" />           <!-- all: hides the status bar at the top of the screen -->
	<preference name="android-minSdkVersion" value="28" /> <!-- android: MIN SDK version supported on the target device. MAX version is blank by default. -->
	<preference name="android-installLocation" value="auto" /> <!-- android: app install location. 'auto' will choose. 'internalOnly' is device memory. 'preferExternal' is SDCard. -->

  <plugin name="cordova-plugin-whitelist"           source="npm" spec="~1.2.1" />

  <access origin="https://forms.gle/UjVSYFpeDgyxP4X38*" />
  <allow-navigation href="https://docs.google.com/forms/*"/>
  
  <allow-intent href="https://forms.gle/UjVSYFpeDgyxP4X38*"/>
    
</widget>